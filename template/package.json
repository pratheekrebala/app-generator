{
    "name": "{{ name }}",
    "version": "0.1.0",
    "description": "{{ description }}",
    "author": "{{ author }}",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt",
        "generate": "nuxt generate",
        "heroku-postbuild": "npm run build",
